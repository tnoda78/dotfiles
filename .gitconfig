[user]
	name = t_noda
	email = t_noda@kogasoftware.com

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch --color
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  lg = log
  ad = add
  tr = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
  ls= log --oneline

[color]
  diff = auto
  tatus = auto
  ranch = auto
